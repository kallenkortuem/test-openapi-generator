{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "5.1.0",
    "generators": { 
      "fetch": {
        "generatorName": "typescript-fetch",
        "output": "#{cwd}/packages/typescript/fetch/#{name}",
        "inputSpec": "https://raw.githubusercontent.com/OAI/OpenAPI-Specification/main/examples/v3.0/petstore.yaml",
        "additionalProperties": {
          "npmName": "restClient",
          "supportsES6": "true",
          "withInterfaces": true,
          "typescriptThreePlus": true
        }
      },
      "axios": {
        "generatorName": "typescript-axios",
        "output": "#{cwd}/packages/typescript/axios/#{name}",
        "inputSpec": "https://raw.githubusercontent.com/OAI/OpenAPI-Specification/main/examples/v3.0/petstore.yaml",
        "additionalProperties": {
          "npmName": "restClient",
          "supportsES6": "true",
          "withInterfaces": true,
          "typescriptThreePlus": true
        }
      }
    }
  }
}
